{
  "swagger": "2.0",
  "info": {
    "title": "API Management service health",
    "version": "1.0"
  },
  "host": "api.parliament.uk",
  "basePath": "/health",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "apiKeyHeader": {
      "type": "apiKey",
      "name": "Ocp-Apim-Subscription-Key",
      "in": "header"
    },
    "apiKeyQuery": {
      "type": "apiKey",
      "name": "subscription-key",
      "in": "query"
    }
  },
  "security": [
    {
      "apiKeyHeader": []
    },
    {
      "apiKeyQuery": []
    }
  ],
  "paths": {
    "/*": {
      "get": {
        "operationId": "Get",
        "responses": {
          "responses": {
            "200": {
              "description": "The request has succeeded"
            }
          }
        }
      }
    }
  }
}